{
  "name": "oss-test",
  "version": "1.0.0",
  "private": true,
  "description": "see requirements",
  "main": "build/server.js",
  "scripts": {
    "start": "node ./build/server.js &",
    "#build": "react-scripts build",
    "#eject": "react-scripts eject",
    "test": "react-scripts test",
    "lint": "./node_modules/.bin/eslint --exit-on-fatal-error --fix --no-cache 'src/**/*.ts' ; ./node_modules/.bin/eslint --exit-on-fatal-error --fix --no-cache 'src/**/*.tsx' ; prettier --write . ; ",
    "build:client": "react-scripts build;",
    "build:server": "webpack --config webpack.config.server.js;",
    "build:app": "react-scripts build; webpack --config webpack.config.server.js; cp ./src/client/default-face.svg build/; cat node_modules/foundation-sites/dist/css/foundation.min.css node_modules/react-day-picker/dist/style.css src/client/App.css >build/app.css; node src/build-tools/build.mjs ",
    "start:storybook": "start-storybook -p 6006 -s public --disable-telemetry --modern --loglevel 3 --no-version-updates ",
    "build:storybook": "build-storybook -s public --disable-telemetry --modern --loglevel 3 "
  },
  "author": "Owen Beresford",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@reach/utils": "^0.17.0",
    "axios": "^0.27.2",
    "axios-es6-class": "^6.0.1",
    "body-parser": "^1.20.0",
    "caniuse-lite": "^1.0.30001451",
    "create-react-app": "^5.0.1",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "focus-trap-react": "^9.0.2",
    "foundation-sites": "^6.7.5",
    "loader-utils": "^3.2.1",
    "mongoose": "^6.5.0",
    "nodemon": "^2.0.19",
    "react": "^18.2.0",
    "react-day-picker": "^8.0.7",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-toggle-component": "^3.0.8",
    "redux": "^4.2.0",
    "svgo": "^2.8.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "jest-dom"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended",
      "standard-react",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "strict": 1
    },
    "env": {
      "browser": true,
      "node": true
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-syntax-typescript": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/mocha": "^10.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-toggle": "^4.0.3",
    "babel-jest": "^29.0.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "e": "^0.2.2",
    "eslint": "^8.23.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.1.1",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "npm": "^8.19.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.2.0",
    "storybook": "^6.5.16",
    "testing-library": "0.0.2",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsify": "^5.0.4",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
